{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6.10.0"
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@core": "./src/core/"
        }
      }
    ]
  ]
}
